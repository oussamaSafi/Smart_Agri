<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agrilogger">

    <application
        android:allowBackup="true"
        android:icon="@drawable/agriloggerlogo_copy"
        android:label="@string/app_name"
        android:roundIcon="@drawable/a_logo_uni_copy"
        android:supportsRtl="true"
        android:theme="@style/Theme.AgriLogger">
        <activity
            android:name=".Soil_graph"
            android:exported="false" />
        <activity
            android:name=".humid_graph"
            android:exported="false"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".temp_graph"
            android:exported="false"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".Dashboard"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>